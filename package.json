    "gatsby-plugin-layout": "^1.1.22",
    "gatsby-plugin-manifest": "^2.2.44",
    "gatsby-plugin-react-helmet-async": "^1.0.16",
    "gatsby-plugin-sass": "^2.1.29",
    "gatsby-plugin-scroll-reveal": "^0.0.6",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-smoothscroll": "^1.0.4",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-remark-relative-images": "^0.3.0",
